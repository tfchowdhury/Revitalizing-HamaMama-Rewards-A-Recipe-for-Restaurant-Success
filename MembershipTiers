CREATE TABLE MembershipTiers (
TIER_ID INT PRIMARY KEY,
TIER_LEVEL VARCHAR(50) NOT NULL,
TIER_DESCRIPTION VARCHAR(255),
MIN_SPENDING NUMBER(9,2) NOT NULL,
VISIT_FREQUENCY INT NOT NULL
);


